{
    "name": "turbo-template",
    "private": true,
    "scripts": {
        "build": "turbo build --concurrency=20",
        "dev": "turbo dev --concurrency=20",
        "lint": "turbo lint --concurrency=20",
        "lint:fix": "turbo lint:fix --concurrency=20",
        "test": "turbo test --concurrency=20",
        "test:watch": "turbo test:watch --concurrency=20",
        "test:ui": "turbo test:ui --concurrency=20",
        "test:e2e": "turbo test:e2e --concurrency=20",
        "test:e2e:ui": "turbo test:e2e:ui --concurrency=20",
        "serve": "turbo serve --concurrency=20",
        "clean": "turbo clean --concurrency=20 && rm -rf node_modules/ .turbo/ .next/",
        "format": "prettier . --write",
        "prepare": "lefthook install",
        "generate": "plop"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
        "@playwright/test": "^1.49.1",
        "@storybook/blocks": "^8.4.7",
        "@storybook/nextjs": "^8.4.7",
        "@storybook/react": "^8.4.7",
        "@storybook/test": "^8.4.7",
        "@storybook/addon-a11y": "^8.4.7",
        "@storybook/addon-console": "^3.0.0",
        "@storybook/addon-essentials": "^8.4.7",
        "@storybook/addon-interactions": "^8.4.7",
        "@storybook/addon-links": "^8.4.7",
        "@storybook/addon-storysource": "^8.4.7",
        "@storybook/react-vite": "^8.4.7",
        "@storybook/test-runner": "^0.19.1",
        "@whitespace/storybook-addon-html": "^6.1.1",
        "@chromatic-com/storybook": "^2.0.2",
        "axe-playwright": "^2.0.2",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@types/node": "^22.10.2",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitejs/plugin-react-swc": "^3.7.2",
        "@vitest/coverage-v8": "^2.1.8",
        "@vitest/ui": "^2.1.8",
        "storybook": "^8.4.7",
        "autoprefixer": "^10.4.20",
        "jsdom": "^25.0.1",
        "lefthook": "^1.10.1",
        "plop": "^4.0.1",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "serve": "^14.2.4",
        "tailwindcss": "^3.4.17",
        "turbo": "^2.3.3",
        "typescript": "^5.7.2",
        "vite": "^5.4.11",
        "vitest": "^2.1.8"
    },
    "packageManager": "pnpm@10.4.1",
    "engines": {
        "node": ">=20"
    },
    "volta": {
        "node": "20.11.0"
    },
    "pnpm": {
        "overrides": {
            "rollup@<2.79.2": ">=2.79.2"
        },
        "onlyBuiltDependencies": [
            "@biomejs/biome",
            "@swc/core",
            "core-js-pure",
            "esbuild",
            "lefthook",
            "sharp"
        ]
    }
}
