{
    "name": "turbo-template",
    "private": true,
    "scripts": {
        "build": "turbo build --concurrency=20",
        "dev": "turbo dev --concurrency=20",
        "lint": "turbo lint --concurrency=20",
        "lint:fix": "turbo lint:fix --concurrency=20",
        "test": "turbo test --concurrency=20",
        "test:watch": "turbo test:watch --concurrency=20",
        "test:ui": "turbo test:ui --concurrency=20",
        "test:e2e": "turbo test:e2e --concurrency=20",
        "test:e2e:ui": "turbo test:e2e:ui --concurrency=20",
        "serve": "turbo serve --concurrency=20",
        "clean": "turbo clean --concurrency=20 && rm -rf node_modules/ .turbo/ .next/",
        "format": "prettier . --write",
        "prepare": "lefthook install",
        "generate": "plop"
    },
    "dependencies": {
        "react": "catalog:react",
        "react-dom": "catalog:react"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@chromatic-com/storybook": "3.2.6",
        "@commitlint/cli": "19.8.1",
        "@commitlint/config-conventional": "19.8.1",
        "@ianvs/prettier-plugin-sort-imports": "4.4.1",
        "@playwright/test": "1.52.0",
        "@storybook/addon-a11y": "8.6.14",
        "@storybook/addon-actions": "8.6.14",
        "@storybook/addon-console": "^3.0.0",
        "@storybook/addon-essentials": "8.6.14",
        "@storybook/addon-interactions": "8.6.14",
        "@storybook/addon-links": "8.6.14",
        "@storybook/addon-storysource": "8.6.14",
        "@storybook/blocks": "8.6.14",
        "@storybook/experimental-nextjs-vite": "^8.6.14",
        "@storybook/nextjs": "8.6.14",
        "@storybook/react": "8.6.14",
        "@storybook/react-vite": "8.6.14",
        "@storybook/test": "8.6.14",
        "@storybook/test-runner": "0.22.0",
        "@testing-library/jest-dom": "6.6.3",
        "@testing-library/react": "16.3.0",
        "@types/node": "22.15.21",
        "@vitejs/plugin-react": "4.5.0",
        "@vitejs/plugin-react-swc": "3.10.0",
        "@vitest/coverage-v8": "2.1.9",
        "@vitest/ui": "2.1.9",
        "@whitespace/storybook-addon-html": "7.0.0",
        "autoprefixer": "10.4.21",
        "axe-playwright": "2.1.0",
        "jsdom": "26.1.0",
        "lefthook": "1.11.13",
        "playwright": "1.52.0",
        "plop": "4.0.1",
        "postcss": "8.5.3",
        "prettier": "3.5.3",
        "prettier-plugin-tailwindcss": "0.6.11",
        "serve": "14.2.4",
        "storybook": "8.6.14",
        "tailwindcss": "3.4.17",
        "turbo": "2.5.3",
        "typescript": "5.8.3",
        "vite": "5.4.19",
        "vitest": "2.1.9"
    },
    "packageManager": "pnpm@10.4.1",
    "engines": {
        "node": ">=20"
    },
    "volta": {
        "node": "20.11.0"
    },
    "pnpm": {
        "overrides": {
            "rollup@<2.79.2": ">=2.79.2",
            "react": "catalog:react",
            "react-dom": "catalog:react"
        },
        "onlyBuiltDependencies": [
            "@biomejs/biome",
            "@swc/core",
            "core-js-pure",
            "esbuild",
            "lefthook",
            "sharp"
        ]
    }
}
